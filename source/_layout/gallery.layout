<div class="gallery-link" id="gallery-link-<%-galleryLinkId-%>" data-id="<%-dataId-%>" data-tag="<%-dataTag-%>" data-href="<%-dataHref-%>" style="background-image:url('<%-galleryPreview-%>')">
    <div class="gallery-overlay">
        <h3 class="gallery-title">
            <%-galleryTitle-%>
        </h3>
        <p class="gallery-description">
            <%-galleryDescription-%>
        </p>
        <div class="gallery-tags">
            <%-galleryTag-%>
        </div>
    </div>
    <span class="gallery-mutil-img"><img src="/icon/gallery/multi-img.svg"></span>
   <!-- <img class="gallery-preview" src="<%-galleryPreview-%>"> -->
</div>